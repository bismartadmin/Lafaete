SELECT DISTINCT "ID_PESSOA","DESC_PESSOA","ID_EMPRESA","ID_SITE","DATA_COMPETENCIA","MAO_DE_OBRA"
FROM (  SELECT ID_PESSOA, DESC_PESSOA, ID_EMPRESA, ID_SITE, MIN(DATA_COMPETENCIA) AS DATA_COMPETENCIA, MAX(MAO_DE_OBRA) AS MAO_DE_OBRA
        FROM IFSINFO.LAF_BI_SALARIOS
        GROUP BY ID_PESSOA, DESC_PESSOA, ID_EMPRESA, ID_SITE
		    ) a
