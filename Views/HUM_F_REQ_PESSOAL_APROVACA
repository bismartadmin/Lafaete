SELECT
ID_REQUISICAO,
ID_ROTA,
TO_DATE(DATA_CADASTRO,'DD/MM/RRRR') DATA_CADASTRO,
NOME_REQUISICAO,
ID_REQUISITANTE,
DESC_REQUISITANTE,
NIVEL_APROVACAO,
TO_DATE(DATA_APROVACAO,'DD/MM/RRRR') DATA_APROVACAO,
TIPO_APROVADOR,
TIPO_APROVACAO,
ID_APROVADOR,
DESC_APROVADOR,
STATUS_APROVACAO,
JUSTIFICATIVA COMENTARIO
FROM IFSLAF.HUM_F_REQ_PESSOAL_APROVACAO
