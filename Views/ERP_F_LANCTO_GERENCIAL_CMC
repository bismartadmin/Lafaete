SELECT
     a.CF$_TIPO_LANCAMENTO  TIPO_LANCAMENTO,
     a.CF$_CODE_F ID_CODE_F_PROJETO,
     CF$_DESC_CODE_F desc_CODE_F_PROJETO,
     a.CF$_CODE_C ID_CODE_C_SITE,
     CF$_DESC_CODE_C desc_CODE_C_SITE,
     a.CF$_TIPO_CUSTO TIPO_CUSTO,
      TO_date('01/'||a.CF$_MES_REFERENCIA_db||'/'||coalesce(a.CF$_ANO_REFERENCIA, '2024'),'DD/MM/YYYY') DATA_REFERENCIA,
     A.CF$_PIS_COFINS_ISS PIS_COFINS_ISS,
     a.CF$_ICMS           ICMS,
     A.CF$_VALOR          VALOR,
     A.CF$_CONTRIBUICAO_SOCIAL CONTRIBUICAO_SOCIAL,
     A.CF$_IMPOSTO_DE_RENDA IMPOSTO_DE_RENDA,
     A.CF$_OVERHEAD OVERHEAD     
FROM ifslaf.LANCTO_GERENCIAL_C_M_C_CLV a
