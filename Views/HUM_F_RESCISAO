select ID_PESSOA,
       ID_SITE,
       DIAS_AVISO,
       TO_DATE(AVISO_PREVIO_INICIO) AVISO_PREVIO_INICIO,
       TO_DATE(AVISO_PREVIO_FIM) AVISO_PREVIO_FIM,
       TO_DATE(DATA_RESCISAO) DATA_RESCISAO,
       TO_DATE(DATA_PAGAMENTO) DATA_PAGAMENTO,
       CAUSA_DEMISSAO
from   ifslaf.laf_rescisao
