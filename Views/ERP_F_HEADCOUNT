select a.ID_PESSOA,
       A.desc_PESSOA,
       TO_DATE(A.DATA_ADMISSAO) DATA_ADMISSAO,
       A.ID_EMPRESA,
       A.ID_CODE_C_SITE,
       A.DESC_CARGO,
       B.CF$_CODE_B ID_CODE_B_CR,
       B.CF$_DESCRICAO_CODE_B DESC_CODE_B_CR,
       A.TIPO_MAO_DE_OBRA,
       A.VALOR_SALARIO 

from  ifslaf.LAF_HEADCOUNT a JOIN IFSLAF.LAF_LOTACAO_HUMANUS_CLV B ON A.ID_LOTACAO = B.CF$_CODIGO_LOTACAO AND A.ID_CODE_C_SITE = B.CF$_CONTRACT
