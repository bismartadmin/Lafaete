SELECT
  CAST(ID_REQUISICAO AS VARCHAR2(50)) ID_REQUISICAO,
  DATA_CADASTRO,
  DATA_REQUISICAO,
  CAST(ID_CADASTRADOR AS VARCHAR2(50))ID_CADASTRADOR,
  CAST(DESC_CADASTRADOR AS VARCHAR2(500)) DESC_CADASTRADOR,
  CAST(QTDE_VAGAS AS VARCHAR2(30)) QTDE_VAGAS,
  CAST(SALARIO_BASE AS VARCHAR2(30)) SALARIO_BASE,
  CAST(JUSTIFICATIVA_VAGA AS VARCHAR2(800)) JUSTIFICATIVA_VAGA ,
  CAST(ID_REQUISITANTE AS VARCHAR2(30)) ID_REQUISITANTE,
  CAST(DESC_REQUISITANTE AS VARCHAR2(500)) DESC_REQUISITANTE,
  CAST(ID_CARGO AS VARCHAR2(30)) ID_CARGO,
  CAST(ID_STATUS AS VARCHAR2(30)) ID_STATUS,
  CAST(DESC_STATUS AS VARCHAR2(50)) DESC_STATUS,
  CAST(ID_EMPRESA AS VARCHAR2(30)) ID_EMPRESA_HUM,
  CAST(ID_SITE AS VARCHAR2(30)) ID_SITE_HUM,
  CAST(ID_LOTACAO AS VARCHAR2(30)) ID_LOTACAO,
  CAST(TIPO_REQUISICAO AS VARCHAR2(30)) TIPO_REQUISICAO,
  CAST(ID_SALARIO AS VARCHAR2(30)) ID_SALARIO,
  CAST(ID_TURNO AS VARCHAR2(30)) ID_TURNO,
  CAST(ID_HORARIO AS VARCHAR2(30)) ID_HORARIO,
  CAST(DATA_CANCELAMENTO AS VARCHAR2(30)) DATA_CANCELAMENTO,
  CAST(ID_USER_CANCELAMENTO AS VARCHAR2(30)) ID_USER_CANCELAMENTO,
  CAST(DESC_USER_CANCELAMENTO AS VARCHAR2(30)) DESC_USER_CANCELAMENTO,
  CAST(JUSTIFICATIVA_CANCELAMENTO AS VARCHAR2(30)) JUSTIFICATIVA_CANCELAMENTO,
  CAST(TIPO_CONTRATACAO AS VARCHAR2(30)) TIPO_CONTRATACAO,
  CAST(ID_CENTRO_CUSTO_PORTAL AS VARCHAR2(30)) ID_CENTRO_CUSTO_PORTAL,
  CAST(DATA_FECHAMENTO AS VARCHAR2(30)) DATA_FECHAMENTO,
  CAST(ID_USER_FECHAMENTO AS VARCHAR2(30)) ID_USER_FECHAMENTO,
  CAST(DESC_USER_FECHAMENTO AS VARCHAR2(30)) DESC_USER_FECHAMENTO,
  CAST(DESC_ANALISTA_RESP AS VARCHAR2(50)) DESC_ANALISTA_RESP
FROM
  IFSLAF.HUM_F_REQ_PESSOAL
