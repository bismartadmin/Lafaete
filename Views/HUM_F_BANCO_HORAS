SELECT ID_PESSOA,
       TIPO_FOLHA,
       DATA_COMPETENCIA,
       TO_CHAR(ID_EMPRESA) ID_EMPRESA,
       TO_CHAR(ID_SITE) ID_SITE,
       TO_DATE(INICIO_VIGENCIA,'DD/MM/RRRR') INICIO_VIGENCIA,
       TO_DATE(FIM_VIGENCIA,'DD/MM/RRRR') FIM_VIGENCIA,
       ID_LOTACAO,
       HORAS_REFERENCIA,
       TO_CHAR(ID_RUBRICA) ID_RUBRICA,
       DESC_RUBRICA
FROM IFSLAF.LAF_BANCO_HORAS_RUBRICA
