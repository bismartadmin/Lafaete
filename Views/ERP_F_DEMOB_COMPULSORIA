select 
       A.STATE AS STATUS,
       A.CF$_ORDER_ID AS ID_ORDEM,
       A.CF$_C_AGREEMENT_ID AS CONTRATO_RECORRENTE,
       A.CF$_LOGISTICS_DATE AS DATA_LOGISTICA,
       A.RMA_NO AS N_ADM,
       A.RMA_LINE_NO AS N_LINHA_ADM,
       A.DATE_REQUESTED AS DATA_SOLICITACAO,
       A.CUSTOMER_NO AS COD_CLIENTE,
       a.CATALOG_NO AS COD_MAT_VENDAS,
       IFSLAF.CUST_ORD_CUSTOMER_API.Get_Name(CUSTOMER_NO) AS NOME_CLIENTE,
       A.CATALOG_DESC AS DESC_MAT_VENDA,
       A.QTY_TO_RETURN AS QTDE_DEVOLVER,
       A.ORDER_NO AS N_ORDEM,
       A.LINE_NO AS N_LINHA,
       A.REL_NO AS N_ENTREGA,
       IFSLAF.CUSTOMER_ORDER_LINE_API.GET_PRIMARY_RENTAL_NO(ORDER_NO,LINE_NO,REL_NO, LINE_ITEM_NO) AS N_LOCACAO,                                                                                                       
       A.CONTRACT AS SITE,
       A.HEAD_STATE AS STATUS_CABECALHO

  from ifslaf.RETURN_MATERIAL_JOIN_CFV A
 where CF$_DESMOB_COMP_DB = '1'
