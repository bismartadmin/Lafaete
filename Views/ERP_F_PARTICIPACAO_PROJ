select 
case CF$_PROJECT when 'BI-INTERNO - BI SMART' then SUBSTR(CF$_PROJECT, 1, INSTR(CF$_PROJECT, '-', INSTR(CF$_PROJECT, '-') + 1) - 2) else SUBSTR(CF$_PROJECT, 1, INSTR(CF$_PROJECT, '-') - 2) end AS ID_PROJETO, 
SUBSTR(CF$_PROJECT, INSTR(CF$_PROJECT, '- ') + 2) AS DESC_PROJETO ,
SUBSTR(CF$_COMPANY, 1, INSTR(CF$_COMPANY, '-') - 2) AS ID_SITE ,
SUBSTR(CF$_COMPANY, INSTR(CF$_COMPANY, '- ') + 2) AS DESC_SITE ,
CF$_PERCENTUAL  AS PORCENTAGEM

from ifslaf.LAF_PORCENT_PART_GRUP_CLV
