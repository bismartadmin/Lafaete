SELECT a.cf$_Projeto ID_CODE_F_PROJETO,      
       TO_date('01/'||a.CF$_MES_REFERENCIA_db||'/'||a.CF$_ANO_REFERENCIA,'DD/MM/YYYY') DATA_REFERENCIA,
       A.CF$_TIPO_CUSTO TIPO_CUSTO,
       A.CF$_MULTA_FGTS PARAMETRO_MULTA_FGTS,
       A.CF$_AVISO_INDENIZADO PARAMETRO_aVISO_INDENIZADO

FROM ifslaf.LAFPROVISOESFOLHA_CLV a
