SELECT a.CF$_TIPO_LANCAMENTO  TIPO_LANCAMENTO,
       a.cf$_Projeto ID_CODE_F_PROJETO,
       A.cf$_Site    SITE,      
       a.CF$_TIPO_CUSTO TIPO_CUSTO,
       TO_date('01/'||a.CF$_MES_REFERENCIA_db||'/'||a.CF$_ANO_REFERENCIA,'DD/MM/YYYY') DATA_REFERENCIA,
       A.CF$_PIS_COFINS_ISS PIS_COFINS_ISS,
       a.CF$_INSS           INSS,
       A.CF$_VALOR          VALOR,
       A.CF$_CONTRIBUICAO_SOCIAL CONTRIBUICAO_SOCIAL,
       A.CF$_IMPOSTO_DE_RENDA IMPOSTO_DE_RENDA,
       A.CF$_OVERHEAD OVERHEAD

FROM ifslaf.LANCTO_GERENCIAL_CLV a
