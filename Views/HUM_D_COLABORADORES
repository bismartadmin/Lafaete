SELECT 
    ID_PESSOA,
    DESC_PESSOA,
    DATA_COMPETENCIA,
    ID_EMPRESA,
    TO_CHAR(ID_SITE) ID_SITE,
    ID_LOTACAO,
    ID_CARGO,
    MAO_DE_OBRA,
    GENERO,
    RESCISAO,
    TO_DATE(DATA_RESCISAO) DATA_RESCISAO,
    TIPO_PESSOA,
    DESC_TIPO_PESSOA,
    COD_HORARIO
FROM IFSLAF.LAF_COLABORADORES
