SELECT DISTINCT(ID_CODE_D_NATUREZA),
DESC_CODE_D_NATUREZA,
CLASSIFICACAO,
(SELECT C.CF$_PACKAGE_GROUPING  FROM IFSLAF.CODE_D_cfv C WHERE C.CODE_D = ID_CODE_D_NATUREZA ORDER BY C.CF$_PACKAGE_GROUPING FETCH FIRST ROW ONLY)  PACOTE
FROM (
select  a.CODE_D ID_CODE_D_NATUREZA,
        A.description DESC_CODE_D_NATUREZA,
        a.CF$_EXPENSE_TYPE CLASSIFICACAO
        from IFSLAF.CODE_D_cfv a
        WHERE a.COMPANY = '01'
        
UNION ALL

select  a.CODE_D ID_CODE_D_NATUREZA,
        A.description DESC_CODE_D_NATUREZA,
        a.CF$_EXPENSE_TYPE CLASSIFICACAO
        from IFSLAF.CODE_D_cfv a
        WHERE a.COMPANY = '41')
