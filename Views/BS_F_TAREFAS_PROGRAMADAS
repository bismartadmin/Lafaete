SELECT JOB_ID ID_BACKGROUND,
       STATE STATUS,
       QUEUE_ID ID_FILA,
       DESCRIPTION DESCRICAO,
       POSTED ENVIADO,
       CREATED CRIADO,
       STARTED INICIADO,
       EXECUTED EXECUTADO,
       SCHEDULE_ID ID_PROGRAMACAO
FROM IFSLAF.DEFERRED_JOB
WHERE QUEUE_ID = 24
AND DESCRIPTION NOT IN (
'ID Programação 33034 LAF_JUR_003-AUTOMACAO_TITULOS_JURIDICO','ID Programação 27254 LAF_TI_007_LOG_IFSLAF','ID Programação 19927 LAF_COM_049_QUEDA_PRE_RESERVA','ID Programação 26454 LAF_CONTRATOS_011',
'ID Programação 17970 LAF_CONTRATOS_007_Flag_pagto','ID Programação 19931 LAF_CONT_002_ADITIVAR_CONTRATO','ID Programação 26895 LAF_FIN_017_Notificacoes_titulos','ID Programação 17969 LAF_FISCAL_001_Impressao_notas',
'ID Programação 16982 Laf_Mkt_010_Cliente_Inativos','LAF_TI_013'
)
ORDER BY POSTED DESC
