SELECT 
    CF$_ID_EMPRESA AS ID_EMPRESA,
    CF$_ID_MATERIAL AS ID_MATERIAL_COMPONENTE,
    CF$_ID_MATERIAL_PAI AS ID_MATERIAL,
    CF$_MAO_DE_OBRA_DB AS DESC_MAO_DE_OBRA,
    CF$_ID_NATUREZA AS  ID_NATUREZA,
    CF$_ID_PROJETO AS ID_PROJETO,
    CF$_MES AS MES,
    CF$_ANO AS ANO,
    CF$_CUSTO_MATERIAL AS CUSTO_MATERIAL,
    CF$_CUSTO_OPERACAO AS CUSTO_OPERAÇÃO
FROM IFSLAF.LAF_DETALHA_PLAN_CLV
