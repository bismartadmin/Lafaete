SELECT ID_PESSOA,
       DESC_PESSOA,
       ID_CARGO,
       TO_DATE(DATA_ADMISSAO,'DD/MM/RRRR') DATA_ADMISSAO,
       TO_DATE(DATA_RESCISAO,'DD/MM/RRRR') DATA_RESCISAO,
       ATIVO_INATIVO,
       DIAS_AVISO,
       TO_DATE(AVISO_PREVIO_INICIO,'DD/MM/RRRR') AVISO_PREVIO_INICIO,
       TO_DATE(AVISO_PREVIO_FIM,'DD/MM/RRRR') AVISO_PREVIO_FIM,
       ID_EMPRESA,
       ID_SITE,
       ID_LOTACAO,
       MAO_DE_OBRA,
       CAUSA_DEMISSAO
FROM IFSLAF.laf_admissao
