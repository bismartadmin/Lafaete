SELECT A.CF$_ID_ATIVIDADE                                                              ID_ATIVIDADE,
       A.CF$_DESCRICAO_ATIVIDADE                                                       DESCRICAO_DA_ATIVIDADE,
       A.CF$_OBJETIVO                                                                  OBJETIVO,
       A.cf$_Lider_Tecnico                                                             LIDER_TECNICO,
       A.cf$_Responsavel                                                               RESPONSAVEL,
       IFSLAF.COMPANY_PERSON_CFP.Get_Cf$_Hour_Wage(A.CF$_RESPONSAVEL_DB)               VALOR_HORA_RESPONSAVEL,
       A.CF$_EQUIPE                                                                    EQUIPE,
       A.CF$_PRIORIDADE                                                                PRIORIDADE,
       A.CF$_COMPLEXIDADE                                                              COMPLEXIDADE,
       A.CF$_STATUS                                                                    STATUS,
       B.person_id                                                                     ID_SOLICITANTE,
       A.cf$_Solicitante                                                               DESC_SOLICITANTE,
       IFSLAF.COMPANY_ORG_API.Get_Org_Name(B.company_id,
       B.org_code)                                                                     AREA_SOLICITANTE,
       A.CF$_DATA_SOLICITACAO                                                          DATA_SOLICITACAO, 
       A.CF$_PROGRESSO AS                                                              PROGRESSO,
       A.CF$_HORAS_PREVISTAS                                                           HORAS_PREVISTAS,
       a.CF$_DATA_INICIO                                                               DATA_INICIO,
       A.CF$_DATA_ENTREGA                                                              DATA_ENTREGA_PREVISTA,
       A.CF$_PRAZO_ORIGINAL                                                            DATA_ENTREGA_ORIGINAL,
       A.CF$_MOTIVO_REAJUSTE                                                           MOTIVO_REAJUSTE,
       A.CF$_HORAS_GASTAS                                                              HORAS_GASTAS,
       A.CF$_DATA_ENTREGA_REAL                                                         DATA_ENTREGA_REAL,
       A.CF$_DATA_CONCLUSAO                                                            DATA_CONCLUSAO,
       a.CF$_ID_EVENTO                                                                 EVENTO,
       A.CF$_PROJETO                                                                   PROJETO,
       A.CF$_CASE_IFS                                                                  CASE_IFS,
       A.CF$_CHAMADO_INTERNO                                                           CHAMADO_INTERNO,
       A.CF$_OBSERVACAO                                                                OBSERVACAO,
       a.CF$_CUSTO_REDUZIDO_ANUAL                                                      CUSTO_REDUZIDO_ANUAL,
       A.CF$_CATEGORIA                                                                 CATEGORIA,
       A.CF$_CLASSIFICACAO                                                             CLASSIFICACAO,
       TO_DATE(a.objversion,'YYYY/MM/DD HH24:MI:SS')                                   ULTIMA_ATUALIZACAO


FROM IFSLAF.LAFPROJETOS_T_I_CLV A
OUTER APPLY (SELECT B.ORG_CODE FROM IFSLAF.COMPANY_PERSON_ALL B WHERE B.objkey = A.cf$_Solicitante_db) B
