SELECT A.EVENT_LU_NAME UNIDADE_LOGICA,
       A.EVENT_ID      EVENTO,
       A.EVENT_DESCRIPTION  DESC_EVENTO,
       A.EVENT_TABLE_NAME   TABELA,
       CASE WHEN A.EVENT_ENABLE = 'TRUE' THEN 'Habilitado' else 'Desabilitado' end      STATUS,
       A.DEFINITION_MODIFIED_DATE        DATA_MODIFICACAO
  FROM IFSLAF.FND_EVENT A
 WHERE A.EVENT_ID LIKE 'LAF%'
 ORDER BY A.EVENT_LU_NAME,
          A.EVENT_ID
