SELECT CASE A.ACCOUNT_TYPE
            WHEN 'REVENUES'    THEN 'RECEITA'
            WHEN 'LIABILITIES' THEN 'PASSIVO'
            WHEN 'ASSETS'      THEN 'ATIVOS'
            ELSE 'DESPESA'
       END                AS TIPO_CONTA_AJUST,
       A.ACCOUNT_TYPE     AS TIPO_CONTA,
       A.VOUCHER_TYPE     AS ID_TIPO_LANCTO,
       IFSLAF.VOUCHER_TYPE_API.GET_DESCRIPTION(A.COMPANY, A.VOUCHER_TYPE) AS DESC_TIPO_LANCTO,
       A.VOUCHER_NO       AS N_LANCTO,
       A.VOUCHER_DATE     AS DATA_LANCTO,
       A.reference_serie  AS SERIE_REFERENCIA,
       A.REFERENCE_NUMBER AS N_REFERENCIA,
       a.reference_row_no AS LINHA_REFERENCIA,
       a.currency_code    AS MOEDA,
       a.parallel_currency_code AS MOEDA_PARALELA,
       A.accounting_year        AS ANO_CONTA,
       A.accounting_period      AS PERIODO_CONTA,
       A.ACCOUNT          AS ID_CODE_A_CONTA,
       A.ACCOUNT_DESC     AS DESC_CODE_A_CONTA,
       A.COMPANY          AS ID_EMPRESA,
       A.ACCOUNT_GROUP    AS GRUPO_CONTA_CONTAB,
       IFSLAF.ACCOUNT_GROUP_API.GET_DESCRIPTION(A.COMPANY, A.ACCOUNT_GROUP) AS DESC_GRUPO_CONTA_CONTAB,
       A.CODE_B           AS ID_CODE_B_CR,
       A.CODE_C           AS ID_CODE_C_SITE,
       A.CODE_D           AS ID_CODE_D_NATUREZA,
       A.CODE_E           AS ID_CODE_E_ATIVO,
       A.CODE_F           AS ID_CODE_F_PROJETO,
       A.CODE_G           AS ID_CODE_G_LINHA,
       A.CODE_H           AS ID_CODE_H_APR_CUSTO,
       A.CODE_I           AS ID_CODE_I_NEGOCIO,
       a.code_j           AS ID_CODE_J,
       A.PARTY_TYPE_ID    AS ID_PARCERIO,
       A.PARTY_NAME       AS DESC_PARCEIRO,
       COALESCE(IFSLAF.IDENTITY_INVOICE_INFO_API.GET_IDENTITY_TYPE(A.COMPANY, CASE A.ACCOUNT_TYPE WHEN 'REVENUES' THEN A.PARTY_TYPE_ID ELSE '' END, 'Cliente'), IFSLAF.IDENTITY_INVOICE_INFO_API.GET_IDENTITY_TYPE(A.COMPANY, CASE WHEN A.ACCOUNT_TYPE IN ('DESP', 'COST') THEN A.PARTY_TYPE_ID ELSE '' END, 'Fornecedor'), 'Externo') AS TIPO_PARCEIRO,
       A.credit_amount    AS CREDITO,
       A.debet_amount     AS DEBITO,
       A.AMOUNT           AS VALOR_CONTAB,
       A.TEXT             AS OBS
  FROM IFSLAF.GEN_LED_VOUCHER_ROW_UNION_QRY A
