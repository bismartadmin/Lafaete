SELECT id_requisicao,
       to_date(data_cadastro,'dd/mm/rrrr') data_cadastro,
       to_date(data_requisicao,'dd/mm/rrrr') data_requisicao,
       id_cadastrador,
       desc_cadastrador,
       qtde_vagas,
       salario_base,
       justificativa_vaga,
       id_requisitante,
       desc_requisitante,
       id_cargo,
       id_status,
       id_empresa_hum,
       id_site_hum,
       id_lotacao,
       tipo_requisicao,
       id_salario,
       id_turno,
       id_horario,
       to_date(data_cancelamento,'dd/mm/rrrr') data_cancelamento,
       id_user_cancelamento,
       desc_user_cancelamento,
       justificativa_cancelamento,
       tipo_contratacao,
       id_centro_custo_portal,
       to_date(data_fechamento,'dd/mm/rrrr') data_fechamento,
       id_user_fechamento,
       desc_user_fechamento
FROM ifslaf.HUM_F_REQ_PESSOAL
